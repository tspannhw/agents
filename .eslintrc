{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "angular": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-unused-vars": 1,
    "new-cap": 1,
    "no-use-before-define": 1,
    "no-undef": 1,
    "no-shadow": 1
  }
}
